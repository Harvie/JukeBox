\pard\plain\ltrpar\qj\s30\itap0{\s30\f0\fs22\lang1029{\*\listtag0}\abinodiroverride\ltrch Takt\'e9\uc1\u382\'9e d\uc1\u283\'eckuji.}{\s30\f0\fs22\lang1029{\*\listtag0}\par}
